{
	"expo": {
		"name": "star_seeker",
		"slug": "star_seeker",
		"version": "0.5.0",
		"orientation": "portrait",
		"icon": "./src/assets/images/orbit-icon.png",
		"scheme": "starseeker",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"splash": {
			"image": "./src/assets/images/orbit-icon.png",
			"resizeMode": "contain",
			"backgroundColor": "#020617"
		},
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./src/assets/images/orbit-icon.png",
				"backgroundColor": "#020617"
			},
			"edgeToEdgeEnabled": true,
			"predictiveBackGestureEnabled": false
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./src/assets/images/orbit-icon.png"
		},
		"plugins": ["expo-router"],
		"experiments": {
			"typedRoutes": true
		}
	}
}
